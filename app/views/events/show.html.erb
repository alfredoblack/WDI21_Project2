
<div class="container">
<% @event.event_images.each do |image, user| %>
  <%= image_tag(image.medium.url) %>
  <% end %>

 
        <%=@event.creator.username %>
     
          <%=@event.comment%>

          <%=@event.attendees.count %> people attending.

          <% @event.attendees.each do |attendee| %>
          <p><%= attendee.first_name %></p>
          <p><%= image_tag (attendee.profile_image.thumb.url) %></p>

          <% end %>
    
    </div>
  
  

  

  <%= link_to "Events", events_path %>
  <% if !current_user.events_attending.include?(@event) %>
    <%= link_to "Attend Event", attend_event_path(@event), method: :post %>
  <% else %>
    <%= link_to "Leave Event", leave_event_path(@event), method: :delete %>

    
  <% end %>

  <%= link_to 'Edit', edit_event_path(@event) %> |
  <%= link_to 'Back', events_path %>

